import pandas as pd

# Sample DataFrame
df = pd.DataFrame({'id': ['A', 'B', 'A', 'C', 'B', 'B', 'D', 'E', 'E', 'E']})

# Count occurrences of each ID
duplicate_counts = df['id'].value_counts().reset_index()

# Rename columns
duplicate_counts.columns = ['id', 'count']

# Filter only IDs that appear more than once
duplicate_counts = duplicate_counts[duplicate_counts['count'] > 1]

# Display results
print(duplicate_counts)
duplicates = df.groupby('id').size().reset_index(name='count')

# Filter IDs that appear more than once
duplicates = duplicates[duplicates['count'] > 1]

print(duplicates)
