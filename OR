from concurrent.futures import ThreadPoolExecutor

def run_stat_multithread(states, start_year=2020, end_year=2024, quarters=(1, 2, 3, 4)):
    params = [(s, y, q) for s in states 
                       for y in range(start_year, end_year + 1) 
                       for q in quarters]
    with ThreadPoolExecutor() as executor:
        executor.map(lambda args: get_STAT(*args), params)
