import pandas as pd

bucket="YOUR_BUCKET_NAME"
key="hothouse.csv"  # or folder/hothouse.csv

path=f"s3://{bucket}/{key}"

df=pd.read_csv(path)

df.head()


import pandas as pd
import s3fs

fs=s3fs.S3FileSystem(
    key="AWS_ACCESS_KEY",
    secret="AWS_SECRET_KEY",
    client_kwargs={"region_name":"us-west-2"}
)

with fs.open("YOUR_BUCKET_NAME/hothouse.csv") as f:
    df=pd.read_csv(f)

df.head()

